package com.sathya.springEx;

public class GreetingImpl {
	@Override
	public void greet() {
       System.out.println("This is the spring first example");
       System.out.println("Ratan is handsome");
	}
	
	public GreetingImpl() {
		System.out.println("GreetingImpl constructor:object created");
	}	

}
